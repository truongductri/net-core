@{
    ViewData["Title"] = "Liên hệ";
    Layout = "_LayoutBootClean";
}
<!-- Sub intro -->
<section id="subintro">
    <div class="container">
        <div class="row">
            <div class="span12">
                <h4>@ViewData["Title"]</h4>
                <h6>Hãy liên hệ với chúng tôi để biết thêm chi tiết</h6>
            </div>

        </div>
    </div>
</section>
<!-- End intro -->
<!-- begin main contain -->
<section id="maincontain">
    <div class="container entry-contact">
        <div class="row">
            <div class="span12">
                <h4 class="entry-contact-title under-line">Địa chỉ công ty Công nghệ và Giải pháp OMEGA</h4>
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3919.0597204718165!2d106.71014541534332!3d10.80673809230094!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x317528a33066f521%3A0xfc01d82dd9ef4c45!2zMzIgxJDGsOG7nW5nIEQ1LCBQaMaw4budbmcgMjUsIELDrG5oIFRo4bqhbmgsIEjhu5MgQ2jDrSBNaW5oLCBWaWV0bmFt!5e0!3m2!1sen!2s!4v1495092212972" width="100%" height="300" frameborder="0" style="border:0" allowfullscreen></iframe>
            </div>
        </div>
        <div class="row">
            <div class="span8 left-column">
                <h4 class="entry-contact-title under-line">Chúng tôi sẵn sàng lắng nghe</h4>
                <form class="form-horizontal form-listen-ideas">
                    <div class="span4">
                        <div class="control-group">
                            <input type="text" id="inputName" name="fullname" placeholder="Tên" class="input-resize" required />
                        </div>
                        <div class="control-group">
                            <input type="email" id="inputEmail" name="email" placeholder="Email" class="input-resize" required />
                        </div>
                        <div class="control-group">
                            <input type="text" id="inputPhone" name="phone" placeholder="Số điện thoại" class="input-resize" required />
                        </div>
                        <div class="control-group">
                            <input type="text" id="inputSubject" name="subject" placeholder="Tiêu đề" class="input-resize" required />
                        </div>
                    </div>
                    <div class="span4">
                        <div class="control-group">
                            <textarea rows="6" id="textareaMessage" name="message" class="textarea-resize" placeholder="Nội dung liên hệ" required></textarea>
                        </div>
                        <div class="control-group text-right">
                            <button type="submit" class="btn btn-flat btn-theme">Send</button>
                        </div>
                    </div>
                </form>
            </div>
            <div class="span4 right-column">
                <h4 class="entry-contact-title under-line">Thông tin liên hệ</h4>
                <table>
                    <tr><td colspan="2">32 Đường D5, Phường 25, Hồ Chí Minh, Việt Nam</td></tr>
                    <tr><td>MST</td><td>0309313833</td></tr>
                    <tr><td>Phone</td><td>(+84-8) 3506 5881 - (+84) 0908 303 609</td></tr>
                    <tr><td>Fax</td><td>(+84-8) 3514 7280</td></tr>
                    <tr><td>Email</td><td>info@omega.com.vn</td></tr>
                </table>
            </div>
        </div>
    </div>
</section>
<!-- end main contain -->
